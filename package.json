{
  "name": "gatsby-starter-contentful-homepage-ts",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "uniform:download-manifest": "uniform context manifest download --output src/lib/uniform/uniform-manifest.json",
    "start": "gatsby develop",
    "develop": "run-s copy:deno uniform:download-manifest develop-gatsby",
    "develop-gatsby": "gatsby develop",
    "build": "run-s copy:deno uniform:download-manifest build-gatsby",
    "build-gatsby": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "gatsby-provision": "gatsby-provision-contentful --contentful-data-path='./scripts/data.json' --space-id=$CONTENTFUL_SPACE_ID --management-token=$CONTENTFUL_MANAGEMENT_TOKEN",
    "setup": "node ./scripts/setup.js",
    "export-content": "contentful space export --space-id=$SPACE_ID --management-token=$MANAGEMENT_TOKEN --export-dir=scripts --content-file=data.json",
    "copy:deno": "shx cp node_modules/@uniformdev/context-edge-netlify/dist/index.deno.js src/lib/uniform",
    "postinstall": "run-s copy:deno"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.15.1",
    "@fontsource/dm-mono": "^4.5.10",
    "@fontsource/dm-sans": "^4.5.9",
    "@uniformdev/cli": "19.176.2-alpha.0",
    "@uniformdev/context": "19.176.2-alpha.0",
    "@uniformdev/context-edge": "19.176.2-alpha.0",
    "@uniformdev/context-edge-netlify": "19.176.2-alpha.0",
    "@uniformdev/context-react": "19.176.2-alpha.0",
    "@vanilla-extract/babel-plugin": "^1.2.0",
    "@vanilla-extract/css": "^1.9.0",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/webpack-plugin": "^2.1.12",
    "gatsby": "^5.0.0",
    "gatsby-plugin-image": "^3.0.0",
    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-sharp": "^5.0.0",
    "gatsby-plugin-vanilla-extract": "^4.0.1",
    "gatsby-source-contentful": "^8.0.0",
    "gatsby-source-filesystem": "^5.0.0",
    "gatsby-transformer-sharp": "^5.0.0",
    "is-absolute-url": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-feather": "^2.0.9",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "chalk": "4",
    "contentful-cli": "^1.15.3",
    "contentful-import": "^8.3.2",
    "gatsby-provision-contentful": "^0.0.5",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^12.3.5",
    "prettier": "2.5.1",
    "shx": "^0.3.4",
    "typescript": "^4.6.2",
    "yargs-parser": "^21.0.0"
  }
}
